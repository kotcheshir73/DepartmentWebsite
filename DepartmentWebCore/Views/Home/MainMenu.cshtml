@using DepartmentWebCore.Helpers

@model List<MenuElementModel>

@{
    Layout = null;
}

<ul class="navig">
    @foreach (var item in Model)
    {
        @if (User.IsInRole(Startup.StudyProcessAuthRole) && item.AdditionalParameters.ContainsKey("StudyProcess") || !item.AdditionalParameters.ContainsKey("StudyProcess"))
        {
            @Html.MenuItem(item)
        }
    }
</ul>